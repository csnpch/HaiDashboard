{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "start cmd /k yarn tailwind && vite",
    "build": "yarn build-tailwind && vite build",
    "build-tsc": "tsc && vite build",
    "build-new": "tsc-silent -p './tsconfig.json' --suppress @ && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tailwind": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/_tailwind.css --watch",
    "build-tailwind": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/_tailwind.css"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^5.1.3",
    "@antv/util": "^3.3.2",
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.9",
    "@mantine/hooks": "^6.0.9",
    "@nivo/sunburst": "^0.83.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons-react": "^2.20.0",
    "antd": "^5.5.1",
    "axios": "^1.3.6",
    "framer-motion": "^10.12.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.10.0",
    "react-zoom-pan-pinch": "^3.0.7",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "^5.0.7"
  },
